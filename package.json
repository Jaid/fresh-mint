        "react-dom": "^16.3.2",
        "webpack": "^4.6.0",
        "webpack-cli": "^2.0.15",
        "webpack-serve": "^0.3.1"
    },
    "dependencies": {
        "@babel/runtime": "^7.0.0-beta.46",
        "lodash": "^4.17.4"
    },
    "scripts": {
        "build-prod": "NODE_ENV=production ./node_modules/.bin/webpack --config config/webpack.babel.js --output-path build",
