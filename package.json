    "dependencies": {
        "@babel/runtime": "^7.0.0-beta.46",
        "and": "^0.0.3",
        "any-shell-escape": "^0.1.1",
        "bash-minifier": "^0.1.2",
        "bootstrap": "^4.1.1",
        "classnames": "^2.2.5",
        "highlight.js": "^9.12.0",
        "lodash": "^4.17.4",
        "rc-switch": "^1.6.0",
        "react": "^16.3.2",
        "react-bootstrap-switch": "^15.5.3",
        "react-dom": "^16.3.2",
        "react-fast-highlight": "^2.2.0",
        "react-jsonschema-form": "^1.0.3",
        "react-switch": "^2.3.2",
        "url": "^0.11.0"
    },
    "scripts": {
        "build-prod": "rm --recursive --force build && NODE_ENV=production ./node_modules/.bin/webpack --config config/webpack.babel.js --output-path build",
        "profile-prod": "NODE_ENV=production ./node_modules/.bin/webpack --config config/webpack.babel.js --output-path build --profile --json > cache/webpackStats.json",
        "build-dev": "NODE_ENV=development ./node_modules/.bin/webpack --config config/webpack.babel.js --output-path build",
        "start": "yarn run start-prod",
        "start-prod": "NODE_ENV=production node --require @babel/register ./node_modules/.bin/webpack-serve --open --config config/webpack.babel.js",
        "start-dev": "USE_WEBPACK_SERVE=true NODE_ENV=development node_modules/.bin/nodemon --watch config/webpack.babel.js --watch config/babel.js --watch config/app.js --watch yarn.lock --exec 'node --require @babel/register ./node_modules/.bin/webpack-serve --config config/webpack.babel.js'",
        "dist-size": "yarn run build-prod && ./node_modules/.bin/dist-size build",
        "bundle-size": "yarn run profile-prod && ./node_modules/.bin/webpack-bundle-analyzer cache/webpackStats.json build",
        "publish": "yarn run build-prod && node_modules/.bin/surge build \"$npm_package_name.j4id.com\"",
        "env": "printenv"
    }
}
